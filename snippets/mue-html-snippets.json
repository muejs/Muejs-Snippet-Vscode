{
    "页面标准结构": {
        "prefix": "ue-html",
        "body": [
            "<!DOCTYPE HTML>",
            "<html>",
            "<head>",
            "    <meta http-equiv=\"Content-Type\" content=\"text/html;charset=UTF-8\" />",
            "    <title>Mue</title>",
            "    <meta name=\"format-detection\" content=\"telephone=no\" />",
            "    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no\">",
            "    <link rel=\"stylesheet\" href=\"css/mue.css\" />",
            "    <link rel=\"stylesheet\" href=\"css/style.css\" />",
            "</head>",
            "<body>",
            "",
            "    <!-- 依赖库 手机调试的js引用顺序如下 -->",
            "    <script src=\"js/zepto.js\"></script>",
            "    <script src=\"js/mue.js\"></script>",
            "    <script>",
            "        mue.ready(function () {",
            "            // 这里写业务及控件初始化",
            "            ",
            "        })",
            "    </script>",
            "</body>",
            "</html>"
        ]
    },

    "mue标准页面结构": {
        "prefix": "ue-page",
        "body": [
            "<div class=\"mue-page mue-box-vertical\">",
            "    <header>",
            "       <div class=\"mue-bar\">",
            "        <div class=\"mue-bar-left\">",
            "            <a class=\"mue-btn\" onclick=\"mue.back();\"><i class=\"icon-back\"></i></a>",
            "        </div>",
            "        <div class=\"mue-bar-main\">Mue页面标题</div>",
            "        <div class=\"mue-bar-right\">",
            "            <!-- 右边按钮 -->",
            "        </div>",
            "       </div>",
            "    </header>",
            "    <main>",
            "        <!-- 正文内容 -->",
            "    </main>",
            "    <footer>",
            "        <!-- 底部内容 -->",
            "    </footer>",
            "</div>"
        ]
    },

    "mue标准页面box布局结构": {
        "prefix": "ue-page-box",
        "body": [
            "<div class=\"mue-page mue-box-vertical\">",
